<div class="header">
  <%= render partial: "header" %>
</div>

<div class="container">

<div class="row">
  <% @candidates.each do |c| %>
    <div class="col-md-3">
      <div class="listbox">
        <div class="candidate-name">
          <a href="/candidates/<%= c['id'] %>"><%= c['name'] %></a><br>
        </div>
        Candidate for <%= c['party_memberships']['2015']['name'] %><br>
      </div>
    </div>
  <% end %>
</div>
