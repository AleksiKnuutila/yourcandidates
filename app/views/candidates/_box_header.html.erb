<!-- HTML for top of each candidate box -->

<% if c['image_url'] %>
  <img src="<%= c['image_url'] %>" class="candidate-image-index">
<% else %>
  <i class="fa fa-user fa-4x candidate-fa-index"></i>
  <!-- <span class="glyphicon glyphicon-user candidate-usericon-index" aria-hidden="true"></span> -->
<% end %>

<div class="candidate-name">
  <a href="/candidates/<%= c['id'] %>"><strong><%= c['name'] %></strong></a><br/>
</div>
